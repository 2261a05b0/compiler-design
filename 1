#include <stdio.h> 
#include <ctype.h> 
#include <string.h> 
int main() { 
char ch; 
printf("Enter text (Ctrl+Z to end):\n"); 
while ((ch = getchar()) != EOF) { 
if (isalpha(ch)) printf("%c : Identifier\n", ch); 
else if (isdigit(ch)) printf("%c : Constant\n", ch); 
else if (ch == '+' || ch == '-' || ch == '*' || ch == '/') printf("%c : Operator\n", ch); 
else if (ch == ' ' || ch == '\n' || ch == '\t'); 
else printf("%c : Special symbol\n", ch); 
} 
return 0; 
}
